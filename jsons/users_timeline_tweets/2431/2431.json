    {
        "user_num_id": 2431,
        "user_id": 1371116112444682242,
        "author": "chur",
        "tweet_num": 47,
        "tweet_id": 1384518687177150464,
        "lang": "en",
        "text": "don't ask coz my heart can only speak in feelings"
    },
    {
        "user_num_id": 2431,
        "user_id": 1371116112444682242,
        "author": "chur",
        "tweet_num": 48,
        "tweet_id": 1383622643337682946,
        "lang": "en",
        "text": "I mean I could but why would I want to"
    },
    {
        "user_num_id": 2431,
        "user_id": 1371116112444682242,
        "author": "chur",
        "tweet_num": 49,
        "tweet_id": 1381619438894604288,
        "lang": "en",
        "text": "you ever been so pissed off you just laugh"
    },
